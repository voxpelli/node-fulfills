{
  "name": "fulfills",
  "version": "2.1.1",
  "license": "MIT",
  "description": "Checks whether or not an object fulfills a specified condition",
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "homepage": "https://github.com/voxpelli/node-fulfills",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-fulfills.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "pegjs condition-parser.pegjs condition-parser.js",
    "check:dependency-check": "dependency-check *.js 'test/**/*.js' --no-dev",
    "check:installed-check": "installed-check -i eslint",
    "check:lint": "eslint .",
    "check": "run-p check:*",
    "prepublish": "npm run build",
    "test:mocha": "cross-env NODE_ENV=test c8 --reporter=lcov --reporter text mocha test/**/*.spec.js",
    "test": "run-s check test:*",
    "watch": "nodemon -e pegjs -x \"npm run build\""
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@voxpelli/eslint-config": "7.0.0",
    "@voxpelli/eslint-config-jsdoc-ts": "^0.3.1",
    "c8": "^7.3.4",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "dependency-check": "^4.1.0",
    "eslint": "^7.11.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^15.0.0",
    "eslint-plugin-es": "^3.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "husky": "^4.0.0",
    "installed-check": "^4.0.0-0",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pegjs": "^0.9.0"
  }
}
