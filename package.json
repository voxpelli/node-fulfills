{
  "name": "fulfills",
  "version": "2.1.1",
  "license": "MIT",
  "description": "Checks whether or not an object fulfills a specified condition",
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "homepage": "https://github.com/voxpelli/node-fulfills",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-fulfills.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "index.d.ts.map",
    "condition-parser.js",
    "condition-parser.d.ts",
    "condition-parser.d.ts.map"
  ],
  "scripts": {
    "build:0": "is-ci || run-s clean",
    "build:1:pegjs": "pegjs condition-parser.pegjs condition-parser.js",
    "build:1:declaration": "tsc -p declaration.tsconfig.json",
    "build:1": "run-p build:1:*",
    "build": "run-s build:*",
    "check:dependency-check": "dependency-check *.js 'test/**/*.js' --no-dev",
    "check:installed-check": "installed-check -i eslint",
    "check:lint": "eslint .",
    "check": "run-p check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts' ! -name 'condition-parser.d.ts')",
    "clean": "run-p clean:*",
    "prepare": "husky install && run-s build",
    "sync-gh-actions": "ghat",
    "test:mocha": "cross-env NODE_ENV=test c8 --reporter=lcov --reporter text mocha test/**/*.spec.js",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*",
    "watch": "nodemon -e pegjs -x \"npm run build\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.20.15",
    "@voxpelli/eslint-config": "^11.0.1",
    "@voxpelli/eslint-config-jsdoc-ts": "^0.3.1",
    "@voxpelli/tsconfig": "^2.0.1",
    "c8": "^7.7.3",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "dependency-check": "^4.1.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-mocha": "^8.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "ghat": "^0.14.0",
    "husky": "^6.0.0",
    "installed-check": "^5.0.0-0",
    "is-ci": "^2.0.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pegjs": "^0.9.0",
    "typescript": "^4.3.2"
  }
}
