{
  "name": "fulfills",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Checks whether or not an object fulfills a specified condition",
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "homepage": "https://github.com/voxpelli/node-fulfills",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-fulfills.git"
  },
  "main": "index.js",
  "scripts": {
    "mocha": "NODE_ENV=test nyc --reporter=lcov --reporter text mocha test/**/*.spec.js",
    "dependency-check": "JS_FILES=\"*.js test/*.js\" && dependency-check . $JS_FILES && dependency-check . $JS_FILES --unused --no-dev",
    "test": "installed-check -e -i eslint && eslint . && npm run --silent build && npm run --silent dependency-check && npm run --silent mocha",
    "build": "pegjs condition-parser.pegjs condition-parser.js",
    "watch": "nodemon -e pegjs -x \"npm run build\"",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "dependency-check": "3.3.0",
    "eslint": "5.12.1",
    "eslint-config-semistandard": "13.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "1.3.1",
    "installed-check": "2.2.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.10",
    "nyc": "13.3.0",
    "pegjs": "0.9.0"
  }
}
